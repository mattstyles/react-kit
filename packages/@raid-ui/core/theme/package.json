  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "lib/rk-ui-theme.esm.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "raid",
    "ui",
    "react-kit",
    "raid-ui",
    "raid-ui-theme"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@size-limit/preset-small-lib": "^4.5.5",
    "standard": "^14.3.4"
  },
  "peerDependencies": {
    "styled-components": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
