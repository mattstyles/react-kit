{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ">2%",
        "loose": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "styled-components",
    "transform-react-require",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ]
  ]
}
